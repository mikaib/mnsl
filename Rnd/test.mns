// input
@input(Position: Vec3)
@input(Colour: Vec4)
@input(TexCoords: Vec2)

// output
@output(Colour: Vec4)
@output(TexCoords: Vec2)

// uniforms
@uniform(MVP: Mat4)

// main
func main() {
    output.Position  = input.Position * uniform.MVP;
    output.Colour    = input.Colour;
    output.TexCoords = input.TexCoords;
}